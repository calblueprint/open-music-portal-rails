html {
    overflow-x: hidden;
}

#competition {
    color: white;
    background: #1896d5;
}

#registration {
    color: #7D7D7D;
    background: transparent;
}

*:focus {
    outline: 0;
}

#container {
    width: 800px;
    height: 1300px;
    margin: auto;
    font-family: 'Open Sans', sans-serif;
}

#sideouterline {
    position:absolute;
    top: 217px;
    left: calc(50% - 400px);
    box-shadow: -2px 0px #EBEBEB;
    width: 100px;
    height: 250px;
    background: transparent;
}

#sideinnerline {
    position:absolute;
    top: 217px;
    left: calc(50% - 250px);
    box-shadow: -2px 0px #EBEBEB;
    width: 100px;
    height: 340px;
    background: transparent;
}

#sideotherouterline {
    position:absolute;
    top: 217px;
    left: calc(50% + 300px);
    box-shadow: 2px 0px #EBEBEB;
    width: 100px;
    height: 340px;
    background: transparent;
    z-index: -1;
}

#edit, #favorite, #message, #print {
    position: absolute;
    top: 217px;
    left: calc(50% + 175px);
    height: 46px;
    width: 46px;
    border-radius: 23px;
    border-style: none;
    background: transparent;
}

#favorite {
    left: calc(50% + 225px);
}

#message {
    left: calc(50% + 275px);
}

#print {
    left: calc(50% + 325px);
}

#edit:hover, #favorite:hover, #message:hover, #print:hover {
    background: #1896d5;
}

#editcon, #favcon, #msgcon, #printcon {
    color: #1896d5;
    height: 46px;
    width: 46px;
    padding-top: 13px;
    padding-right: 12px;
}

#editcon:hover, #favcon:hover, #msgcon:hover, #printcon:hover {
    color: white;
}

#stitle {
    font-family: 'Montserrat', sans-serif;
    font-size: 45px;
    margin-left: -3px;
    padding-bottom: 30px;
    width: 500px;
    color: #1896d5;
}


#infobox {
    position:absolute;
    top: 222px;
    left: calc(50% - 250px);
    width: 650px;
    height: 400px;
    padding-left: 50px;
    font-size: 15px;
    line-height: 25px;
    background: transparent;
    z-index: -1;
}

#info {
    float: left;
    color: #ACACAC;
    width: 150px;
    background: transparent;
}

#details {
    float: left;
    padding-left: 30px;
    color: #363636;
    width: 350px;
    background: transparent;
}

.stab {
    position: absolute;
    left: calc(50% - 400px);
    font-size: 15px;
    height: 50px;
    width: 150px;
    border-style: none;
    color: #363636;
    background: transparent;
    transition: color 0.5s;
    -webkit-transition: color 0.5s;
}

.stab:hover {
    color: #1896d5;
    transition: color 0.25s;
    -webkit-transition: color 0.25s;
}

#compete {
    top: 217px;
}

#description {
    color: white;
    background: #1896d5;
    top: 267px;
}

#contact {
    top: 317px;
}

#pieces {
    top: 367px;
}

#notes {
    top: 417px;
}