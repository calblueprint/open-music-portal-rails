.admin_link {
  color: #0000FF;
}

th {
  width: 200px;
  height: 50px;
  background: #2F2F2F;
  border: 5px solid white;
  padding: 25px;
  color: white;
  font-family: 'Roboto', sans-serif;
}

td {
  width: 200px;
  height: 100px;
  background: #EBEBEB;
  border: 5px solid white;
  padding: 25px;
  font-family: 'Roboto', sans-serif;
}

.admin-back-button {
  font-family: 'Roboto', sans-serif;
  font-size: 14px;
  margin-right: -10px;
  color: black;
  opacity: 0.5;
  background: transparent;
  z-index: 2;
  transition: 0.25s;
  -webkit-transition: 0.25s;
}
