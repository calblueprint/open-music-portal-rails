.list_user_labels {
  list-style: none;
  padding: 0;
  color: #ACACAC;
}

.list_user_values {
  list-style: none;
  padding: 0;
}

#user_profile {
  min-height: 100%;
  height: 100%;
}

#main_user_info {
  height: 100%;
  border-right: 2px solid #EBEBEB;
  border-left: 2px solid #EBEBEB;
  padding-left: 40px;
}

#user_sidebar {
  height: 100%;
  border-left: 2px solid #EBEBEB;
}

.profile_component_links {
  list-style: none;
  text-align: center;
  padding: 0;
}

.profile_component_links li {
  padding-top: 20px;
  padding-bottom: 20px;
}

.user_input_label {
  height: 30px;
}

.submit {
  color: black;
  border-style: none;
  background: #EBEBEB;
  width: 100%;
  height: 60px;
  margin-top: 20px;
  margin-bottom: 20px;
}

.submit:hover {
  background: #1896d5;
  color: white;
}
