.list_user_labels {
  list-style: none;
  padding: 0;
  font-family: 'Roboto', sans-serif;
  color: #ACACAC;
}

.list_user_values {
  list-style: none;
  padding: 0;
  font-family: 'Roboto', sans-serif;
}

.reg_body {
  margin: auto;
  text-align: center;
}

#reg_box {
  float: right;
  width: 340px;
  height: 310px;
  padding: 15px;
  margin-right: 20px;
  background: #EBEBEB;
  color: black;
  font-size: 50px;
}

#user_profile {
  min-height: 100%;
  height: 100%;
}

#main_user_info {
  height: 100%;
}

.profile_component_links {
  list-style: none;
  text-align: center;
  padding: 0;
}

.profile_component_links li {
  padding-top: 20px;
  padding-bottom: 20px;
}

.user_input_label {
  height: 30px;
  font-size: 15px;
  padding-top: 3px;
  margin-bottom: 10px;
}

.user_input_value {
  height: 30px;
  font-size: 15px;
  margin-bottom: 10px;
}

.submit {
  font-family: Montserrat, sans-serif;
  color: #2F2F2F;
  border-style: none;
  background: white;
  width: 222px;
  height: 35px;
  margin-left: 0px;
  margin-top: 30px;
  margin-bottom: 40px;
  border: 1px solid #2F2F2F;
  transition: 0.25s;
  -webkit-transition: 0.25s;
}

.submit:hover {
  background: #EBEBEB;
  color: black;
}

.form_label {
  font-family: 'Roboto', sans-serif;
  display: block;
}

input.login_text_field {
  font-family: 'Roboto', sans-serif;
  margin-bottom: 20px;
  height: 50px;
  width: 300px;
}

.user_greeting {
  height: 40px;
}