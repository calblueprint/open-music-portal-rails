.container {
  width: 1000px;
}

.navbar-nav > li {
  height: 41px;
}

.nav > li > a:hover, .nav > li > a:focus {
  background-color: transparent;
}

.navbar-nav > li > a {
  padding-top: 10px;
  padding-left: 40px;
  padding-right: 40px;
}

.navbar-toggle {
  padding: 2px 6px 3px;
  border-radius: 0px;
  margin-top: 0px;
  margin-bottom: 0px;
}

@media (min-width: 768px) and (max-width: 992px) {
  .navbar-nav > li > a {
    padding-left: 15px;
    padding-right: 15px;
  }
}
