/*WINNERS*/
.year_link {
  float: left;
  width: 285px;
  margin: 0px;
  padding: 0px;
  margin-top: 50px;
  margin-bottom: 50px;
  background: white;
  cursor: pointer;
  text-align: center;
  font-family: Montserrat, sans-serif;
  color: #EBEBEB;
  font-size: 50px;
  transition: 0.25s;
  -webkit-transition: 0.25s;
  -webkit-user-select: none; /* Chrome/Safari */        
  -moz-user-select: none; /* Firefox */
  -ms-user-select: none; /* IE10+ */
}

.year_link:hover {
  color: #2F2F2F;
}

