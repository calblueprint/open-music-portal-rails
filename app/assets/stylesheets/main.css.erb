#subheading {
    font-size: 24px;
}
#heading {
    font-size: 48px;
}
#link {
    background: #de6868;
    float: right;
    padding: 10px 20px;
    cursor: pointer;
    margin-top: 10px;
}
#container {
    position: absolute;
    width: 100%;
    height: 500px;
    top: 50%;
    margin-top: -269px;
}
#content {
    width: 900px;
    margin-left: auto;
    margin-right: auto;
    overflow: hidden;
}
#carousel-prev {
    position: absolute;
    width: 40px;
    height: 40px;
    padding: 10px;
    background: rgba(0, 0, 0, 0.5);
    top: 50%;
    margin-top: -30px;
    cursor: pointer;
    -webkit-transition: linear 0.2s;
}
#carousel-next {
    position: absolute;
    width: 40px;
    height: 40px;
    padding: 10px;
    background: rgba(0, 0, 0, 0.5);
    top: 50%;
    right: 0px;
    margin-top: -30px;
    cursor: pointer;
    -webkit-transition: linear 0.2s;
}
#carousel-prev:hover {
    background: rgba(0, 0, 0, 0.9);
}
#carousel-next:hover {
    background: rgba(0, 0, 0, 0.9);
}

