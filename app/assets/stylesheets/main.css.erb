*:focus {
  outline: 0;
}

a:hover {
  text-decoration: none;
}

.container {
  padding: 0px;
  padding-top: 45px;
}

p {
  font-size: 14px;
  line-height: 20px;
  padding: 0px;
  font-family: 'Roboto', sans-serif;
}

.title {
  font-size: 24px;
  line-height: 20px;
  font-family: 'Josefin Sans', sans-serif;
  width: 100%;
  height: 50px;
}

h4.title {
  padding-top: 20px;
}

#home_page_nav ul.dropdown-menu li {
  margin: auto;
  z-index: 2;
  display: list-item;
}

#home_page_nav ul.dropdown-menu li.divider {
  height: 1px;
  margin: 9px 0;
  overflow: hidden;
  background-color: #e5e5e5;
}

input[type=text], input[type=password] {
  border-style: none;
  border: 1px solid #cdcdcd;
  padding-left: 10px;
  margin-bottom: 5px;
  height: 30px;
  padding-top: 3px;
}

.content_container {
  position: absolute;
  top: 85px;
  width: 100%;
}

ul.no-bullets {
  list-style: none;
}

ul.with-buttons li {
  margin: 10px;
}

a.no-highlight {
  color: white;
}

a.no-highlight:hover {
  color: white;
}

a.gray-highlight {
  color: #EBEBEB;
}

a.gray-highlight:hover {
  color: #2F2F2F;
}

/* WHAT IS THIS OMEGEH*/
#errors_container {
  padding-top: 0px;
}

/*ABOUT*/

#credits {
  padding-top: 35px;
  text-align: center;
}

.about_blurb {
  text-align: center;
}

#images {
  width: 100%;
  height: 500px;
  background: black;
  margin-bottom: 40px;
}

#credits_back {
  position: absolute;
  top: 990px;
  left: 0px;
  height: 650px;
  width: 100%;
  background: #EBEBEB;
}

#credits_title {
  padding-top: 50px;
  text-align: center;
}

/*WINNERS*/
.year_link {
  float: left;
  width: 285px;
  margin: 0px;
  padding: 0px;
  margin-top: 100px;
  background: white;
  cursor: pointer;
  text-align: center;
  font-family: Montserrat, sans-serif;
  color: #EBEBEB;
  font-size: 50px;
}

.year_link:hover {
  color: #2F2F2F;
}

/*BIOS*/

.bio_row {
  margin-top: 50px;
  margin-bottom: 50px;
}

.bio_pic {
  width: 300px;
  height: 280px;
  background: black;
  border-right: solid 20px #EBEBEB;
}

.bio_blurb {
  padding: 50px;
}

.gold {
  border-right: solid 20px #ffd700;
}

.silver {
  border-right: solid 20px #CCCCCC;
}

.bronze {
  border-right: solid 20px #5B391E;
}

/*COMPETITION*/
.comp_link {
  float: left;
  height: 150px;
  width: 290px;
  margin: 0px;
  padding-top: 45px;
  margin-top: 10px;
  cursor: pointer;
  text-align: center;
  font-family: Montserrat, sans-serif;
  color: #EBEBEB;
  background: white;
  font-size: 40px;
}

.comp_link:hover {
  color: #2F2F2F;
}

#loc_images {
  position: absolute;
  top: 300px;
  left: 0px;
  height: 400px;
  width: 100%;
  background-color: black;
  background-size: cover;
  background-position: 0px 0px;
  border-top: solid 20px #EBEBEB;
}

#loc {
  padding: 30px;
  width: 400px;
  font-family: Roboto, sans-serif;
  color: #2F2F2F;
  font-size: 30px;
  line-height: 25px;
  background-color: rgba(255, 255, 255, 0.5);
}
