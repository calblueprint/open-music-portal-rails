*:focus {
  outline: 0;
}

a:hover {
  text-decoration: none;
}

.container {
  padding: 0px;
  padding-top: 45px;
}

.inverted_color {
  color: white;
}

.blurb {
  margin: auto;
  text-align: center;
  line-height: 35px;
}

p {
  font-size: 14px;
  line-height: 20px;
  padding: 0px;
  font-family: 'Roboto', sans-serif;
}

.title {
  font-size: 24px;
  line-height: 20px;
  font-family: 'Josefin Sans', sans-serif;
  width: 100%;
  height: 50px;
}

h4.title {
  padding-top: 20px;
}

#home_page_nav ul.dropdown-menu li {
  margin: auto;
  z-index: 2;
  display: list-item;
}

#home_page_nav ul.dropdown-menu li.divider {
  height: 1px;
  margin: 9px 0;
  overflow: hidden;
  background-color: #e5e5e5;
}

input[type=text], input[type=password] {
  border-style: none;
  border: 1px solid #cdcdcd;
  padding-left: 10px;
  margin-bottom: 5px;
  height: 30px;
  padding-top: 3px;
}

.content_container {
  position: absolute;
  top: 85px;
  width: 100%;
}

ul.no-bullets {
  list-style: none;
}

ul.with-buttons li {
  margin: 10px;
}

a.no-highlight {
  color: white;
}

a.no-highlight:hover {
  color: white;
}

a.gray-highlight {
  color: #EBEBEB;
}

a.gray-highlight:hover {
  color: #2F2F2F;
}

/* WHAT IS THIS OMEGEH*/
#errors_container {
  padding-top: 0px;
}

/*ABOUT*/

