*:focus {
  outline: 0;
}

a:hover {
  text-decoration: none;
}

.title {
  color: white;
}

.container {
  padding: 0px;
  padding-top: 45px;
  width: 100%;
}

p {
  font-size: 14px;
  line-height: 20px;
  font-family: 'Roboto', sans-serif;
}

input[type=text], input[type=password] {
  border-style: none;
  border: 1px solid #cdcdcd;
  padding-left: 10px;
  margin-bottom: 5px;
  height: 30px;
  padding-top: 3px;
}

.content_container {
  position: absolute;
  top: 85px;
  width: 100%;
}

/* WHAT IS THIS OMEGEH*/
#errors_container {
  padding-top: 0px;
}

/*ABOUT*/
#images {
  position: absolute;
  top: 85px;
  left: 0px;
  height: 420px;
  width: 100%;
  background-color: black;
  background-size: cover;
  background-position: 0px 0px;
  border-bottom: solid 20px #EBEBEB;
}

#about_container {
  margin-top: 400px;
}

/*WINNERS*/
.year_link {
  float: left;
  width: 285px;
  margin: 0px;
  padding: 0px;
  margin-top: 100px;
  background: white;
  cursor: pointer;
  text-align: center;
  font-family: Montserrat, sans-serif;
  color: #EBEBEB;
  font-size: 50px;
}

.year_link:hover {
  color: #2F2F2F;;
}