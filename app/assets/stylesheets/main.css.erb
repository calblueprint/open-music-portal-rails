*:focus {
  outline: 0;
}

a:hover {
  text-decoration: none;
}

.container {
  padding: 0px;
  padding-top: 45px;
}

p {
  font-size: 14px;
  line-height: 20px;
  padding: 0px;
  font-family: 'Roboto', sans-serif;
}

.title {
  font-size: 30px;
  line-height: 20px;
  font-family: 'Josefin Sans', sans-serif;
  width: 100%;
  height: 50px;
}

input[type=text], input[type=password] {
  border-style: none;
  border: 1px solid #cdcdcd;
  padding-left: 10px;
  margin-bottom: 5px;
  height: 30px;
  padding-top: 3px;
}

.content_container {
  position: absolute;
  top: 85px;
  width: 100%;
}

ul.no-bullets {
  list-style: none;
}

ul.with-buttons li {
  margin: 10px;
}

a.no-highlight {
  color: white;
}

a.no-highlight:hover {
  color: white;
}

/* WHAT IS THIS OMEGEH*/
#errors_container {
  padding-top: 0px;
}

/*ABOUT*/
#images {
  position: absolute;
  top: 85px;
  left: 0px;
  height: 420px;
  width: 100%;
  background-color: black;
  background-size: cover;
  background-position: 0px 0px;
  border-bottom: solid 20px #EBEBEB;
}

#about_container {
  margin-top: 400px;
}

/*WINNERS*/
.year_link {
  float: left;
  width: 285px;
  margin: 0px;
  padding: 0px;
  margin-top: 100px;
  background: white;
  cursor: pointer;
  text-align: center;
  font-family: Montserrat, sans-serif;
  color: #EBEBEB;
  font-size: 50px;
}

.year_link:hover {
  color: #2F2F2F;
}

@media (min-width: 768px) and (max-width: 992px) {
  .navbar-nav > li > a {
    padding-left: 15px;
    padding-right: 15px;
  }
}

/*BIOS*/

.bio_row {
  margin-top: 50px;
  margin-bottom: 50px;
}

.bio_pic {
  width: 300px;
  height: 280px;
  background: black;
  border-right: solid 20px #EBEBEB;
}

.bio_blurb {
  padding: 50px;
}

.gold {
  border-right: solid 20px #ffd700;
}

.silver {
  border-right: solid 20px #CCCCCC;
}

.bronze {
  border-right: solid 20px #5B391E; 
}

/*COMPETITION*/
.comp_link {
  float: left;
  height: 150px;
  width: 290px;
  margin: 0px;
  padding-top: 45px;
  margin-top: 10px;
  cursor: pointer;
  text-align: center;
  font-family: Montserrat, sans-serif;
  color: #EBEBEB;
  background: white;
  font-size: 40px;
}

.comp_link:hover {
  color: #2F2F2F;
}

#loc_images {
  position: absolute;
  top: 300px;
  left: 0px;
  height: 400px;
  width: 100%;
  background-color: black;
  background-size: cover;
  background-position: 0px 0px;
  border-top: solid 20px #EBEBEB;
}

#loc {
  padding: 30px;
  width: 400px;
  font-family: Roboto, sans-serif;
  color: #2F2F2F;
  font-size: 30px;
  line-height: 25px;
  background-color: rgba(255, 255, 255, 0.5);
}
