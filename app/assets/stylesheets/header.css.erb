#main_header {
  height: 50px;
  background: rgba(0,0,0,0.5);
}

#main_container {
  padding: 0px;
  margin: 50px;
  margin-top: 15px;
}

.tab {
  display: inline-block;
  font-family: 'News Cycle', sans-serif;
  font-size: 25px;
  color: white;
  opacity: 0.5;
  background: transparent;
  z-index: 1;
  margin-left: -20px;
  margin-right: -20px;
}

.tab:hover {
  color: white;
  opacity: 1;
}

#collapse-button {
  position: fixed;
  font-size: 16px;
  color: white;
  top: 0px;
  right: 0px;
  height: 40px;
  width: 40px;
  border-style: none;
  background: transparent;
  z-index: 3;
}

