<table>
  <tr>
    <th>ID</th>
    <th>Last Name</th>
    <th>First Name</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.last_name %></td>
    <td><%= user.first_name %></td>
    <td><%= link_to "Show", user %></td>
    <td><%= link_to "Edit", edit_user_path(user) %></td>
  </tr>
  <% end %>
</table>
<%= will_paginate(@users, :renderer => WillPaginate::ActionView::LinkRenderer) %>
