<li class="dropdown">
<a href="#" class="dropdown-toggle tab" data-toggle="dropdown">DONATE<b class="caret"></b></a>
  <ul class="dropdown-menu">
    <li><%= link_to 'Donor Levels', donor_levels_path %></li>
    <li><%= link_to 'Sponsorship Levels', sponsorship_levels_path %></li>
    <li><%= link_to 'Current Sponsors', current_sponsors_path %></li>
